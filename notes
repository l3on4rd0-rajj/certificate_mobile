import pandas as pd

# Carregar as planilhas
df1 = pd.read_csv('planilha1.csv')  # Substitua pelo caminho da sua Planilha 1
df2 = pd.read_csv('planilha2.csv')  # Substitua pelo caminho da sua Planilha 2

# Renomear as colunas para facilitar a manipulação
df1.columns = ['Projeto', 'MS', 'Fortify']
df2.columns = ['Project Name', 'Latest Version']

# Criar uma nova coluna 'Status' no df1 com valor padrão 'nok'
df1['Status'] = 'nok'

# Verificar se os valores de 'MS' em df1 estão em 'Project Name' em df2
df1.loc[df1['MS'].isin(df2['Project Name']), 'Status'] = 'ok'

# Salvar o resultado em um novo CSV
df1.to_csv('resultado_final.csv', index=False)

print("Processo concluído. O arquivo 'resultado_final.csv' foi gerado.")
