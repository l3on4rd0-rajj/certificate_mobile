
import requests
import json

# URL do endpoint da API
url = "https://sua-api.com/projects"  # ⬅️ Substitua pela URL real do endpoint

# Cabeçalhos da requisição
headers = {
    "Content-Type": "application/json",
    "Authorization": "Bearer SEU_TOKEN_AQUI"  # ⬅️ Substitua pelo token real, se necessário
}

# Corpo da requisição
payload = {
    "name": "0.0.0",
    "description": "inicialização de um novo projeto já na versão 0.0.0",
    "active": True,
    "committed": True,
    "project": {
        "name": "adp-java-Integ-ADP-GerencialM",
        "description": "Project for GETNET BR",
        "issueTemplateId": "Prioritized-HighRisk-Project-Template"
    }
}

# Fazendo a requisição POST
response = requests.post(url, headers=headers, data=json.dumps(payload))

# Exibindo o resultado
print("Status Code:", response.status_code)
print("Resposta:")
print(response.text)
