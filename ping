import requests
import json

# URL correta (sem espaços)
url = "https://ssc.santander.fortifyhosted.com/api/v1/projectVersions/"

# Cabeçalhos da requisição
headers = {
    "Authorization": "FortifyToken SEU_TOKEN_AQUI",  # Substitua pelo token real
    "Content-Type": "application/json",
    "Cookie": "SSC_AUTH_COOKIE=SEU_COOKIE_AQUI"      # Se necessário, insira o cookie
}

# Corpo da requisição
data = {
    "name": "0.0.0",
    "description": "inicialização de um novo projeto já na versão 0.0.0",
    "active": True,
    "committed": True,
    "project": {
        "name": "GetCripto",
        "description": "Project for GETNET BR",
        "issueTemplateId": "Prioritized-HighRisk-Project-Template"
    }
}

# Enviando a requisição
response = requests.post(url, headers=headers, json=data)

# Exibindo a resposta
print("Status Code:", response.status_code)
print("Response Body:", response.text)
